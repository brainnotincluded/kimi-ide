{
  "name": "kimi-ide",
  "version": "0.1.0",
  "description": "AI-first IDE - Cursor alternative powered by Kimi",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && webpack --mode production",
    "dev": "concurrently \"tsc -w\" \"webpack --mode development --watch\" \"electron .\"",
    "start": "electron .",
    "package": "electron-builder",
    "package:mac": "electron-builder --mac"
  },
  "dependencies": {
    "@electron/remote": "^2.1.0",
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@monaco-editor/react": "^4.6.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "electron-store": "^8.1.0",
    "marked": "^12.0.0",
    "monaco-editor": "^0.45.0",
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "react-resizable-panels": "^1.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0",
    "shiki": "^1.0.0",
    "vscode-jsonrpc": "^8.2.0",
    "vscode-languageclient": "^9.0.1",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-syntax-highlighter": "^15.5.11",
    "concurrently": "^8.2.0",
    "css-loader": "^6.11.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "html-webpack-plugin": "^5.6.6",
    "mini-css-extract-plugin": "^2.10.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "build": {
    "appId": "com.kimi.ide",
    "productName": "Kimi IDE",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "assets/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    }
  }
}
