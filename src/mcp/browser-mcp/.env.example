# Trench Browser MCP Configuration

# Default archive output directory
TRENCH_OUTPUT_DIR=./archives

# Default replay server port
TRENCH_REPLAY_PORT=8080

# Default replay server host
TRENCH_REPLAY_HOST=localhost

# Browser settings
TRENCH_BROWSER_TIMEOUT=30000
TRENCH_BROWSER_CONCURRENCY=5

# Archive settings
TRENCH_DEFAULT_FORMAT=trench
TRENCH_COMPRESSION_LEVEL=6

# Logging
TRENCH_LOG_LEVEL=info

# MCP Server transport (stdio or sse)
MCP_TRANSPORT=stdio
MCP_PORT=3000
